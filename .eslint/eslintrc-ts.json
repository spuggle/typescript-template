{
  "env": {},
  "extends": "./eslintrc-base.json",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {},
  "plugins": [ "@typescript-eslint/eslint-plugin" ],
  "overrides": [{
    "files": [ "*.ts", "*.tsx" ],
    "excludedFiles": [ "*.js", "*.jsx", "*.mjs" ],
    "rules": {
      "consistent-return": "off",
      "no-bitwise": "off",
      "int32Hint": "off"
    }
  }]
}

    "eslint": "^7.32.0",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "typescript": "^4.3.5"
  },
  "type": "module",
  "engines": {
    "node": "14.x.x"
  },
  "commitlint": {
    "extends": [
      "./.github/configs/commitlint.json"
    ]
  },
  "eslintConfig": {
    "extends": "./.eslint/eslintrc-node.json",

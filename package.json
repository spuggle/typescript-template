{
  "name": "typescript-template",
  "version": "0.1.0",
  "description": "My TypeScript starter template",
  "private": true,
  "author": "spuggle",
  "license": "Apache-2.0",
  "homepage": "https://github.com/spuggle/typescript-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spuggle/typescript-template.git"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "bugs": {
    "url": "https://github.com/spuggle/typescript-template/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "ttsc",
    "build:watch": "ttsc --pretty --sourceMap --watch",
    "lint": "eslint src/",
    "lint:commit": "git log -1 --pretty=format:'%B' | commitlint",
    "lint:fix": "eslint --fix src/",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "dependencies": {
    "@zerollup/ts-transform-paths": "^1.7.18",
    "ttypescript": "^1.5.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "husky": "^7.0.2",
    "typescript": "^4.3.5"
  },
  "type": "commonjs",
  "engines": {
    "node": "14.x.x"
  },
  "commitlint": {
    "extends": [
      "./.github/configs/commitlint.json"
    ]
  },
  "eslintConfig": {
    "extends": "./.eslint/all.json",
    "root": true,
    "parserOptions": {
      "ecmaVersion": "2018"
    }
  }
}
